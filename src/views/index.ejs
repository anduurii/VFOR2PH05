<%- include('partials/header') %>

<h1>Vinsælar myndir</h1>
<div class="card-grid">
    <% movies.forEach(movie => { %>
    <div class="card">
        <img src="<%= movie.image %>" alt="<%= movie.title %>">
        <h3><%= movie.title %></h3>
        <p><%= movie.year %></p>
        <a href="/movie/<%= movie.id %>">Skoða nánar</a>
    </div>
    <% }) %>
</div>

<%- include('partials/footer') %>